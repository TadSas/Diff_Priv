WindowManager:
    MainWindow:
    FirstWindow:
    SecondWindow:

<MainWindow>:
    name: "main"

    BoxLayout:
        orientation: 'vertical'
        size: root.width, root.height

        GridLayout:
            cols: 5
            rows: 1
            padding: 10
            spacing: 5
            size_hint: (1, .15)

            Widget:
            Widget:
            Label:
                text: "Լոկալ և Գլոբալ Դիֆերենցիալ Գաղտնիություն"
                font_name: 'GHEAGrpalatReg.otf'
                font_size: 36
                color: [28/255.0, 72/255.0, 100/255.0, 1]
            Widget:
            Widget:

        GridLayout:
            cols: 5
            rows: 2
            padding: 10
            spacing: 5
            size_hint: (1, .2)

            Widget:
            Widget:
            Widget:
            Widget:
            Widget:

            Image:
                source: 'Left_arrow.png'
            Button:
                text: "Լոկալ Դիֆերենցիալ\nԳաղտնիություն"
                font_name: 'GHEAGrpalatReg.otf'
                background_normal: ''
                background_color: [45/255.0, 129/255.0, 131/255.0, 1]
                halign: 'center'
                valign: 'center'
                on_release:
                    app.root.current = "first"
                    root.manager.transition.direction = "right"
            Widget:
            Button:
                text: "Գլոբալ Դիֆերենցիալ\nԳաղտնիություն"
                font_name: 'GHEAGrpalatReg.otf'
                background_normal: ''
                background_color: [45/255.0, 129/255.0, 131/255.0, 1]
                halign: 'center'
                valign: 'center'
                on_release:
                    app.root.current = "second"
                    root.manager.transition.direction = "left"
            Image:
                source: 'Right_arrow.png'

        GridLayout:
            cols: 1
            rows: 1
            padding: 25
            size_hint: (1, .5)

            Image:
                source: 'main_window.png'

